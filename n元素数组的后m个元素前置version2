#include<stdio.h>
#define N 20
int main()
{
	int number[N],n,m,i;
	printf("the total numbers n is:");
	scanf("%d",&n);
	printf("m is:");
	scanf("%d",&m);
	printf("please input items ï¼š");
	for(i=0;i<n;i++)
		scanf("%d",&number[i]);
	move(number,n,m);
	for(i=0;i<n;i++)
		printf("%d,",number[i]);
}
move(int array[20],int n,int m)
{
	int *p,array_end;
	array_end=*(array+n-1);
	for(p=array+n-1;p>array;p--)
		*p=*(p-1);
	*array=array_end;
	m--;
	if(m>0) 
 		move(array,n,m);
}

	
